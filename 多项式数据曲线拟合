clear;
clc;
x = 0:1:10;
y = [538.548513,526.079113,520.707955,517.713697,514.836850,512.093568,509.601099,507.291466,505.162750,503.212452,501.436809];
plot(x,y,'r*');
hold on;
% 使用polyfit(x,y,n) xy为数据点，x必须为单调的，n为拟合多项式的阶数
% 返回值为n+1个多项式系数
p = polyfit(x,y,4);

x2 = 0:0.1:10;
y2 = polyval(p,x2);
plot(x,y,'o',x2,y2);
grid on
s = sprintf('y = (%.1f)x^4 + (%.1f)x^3 + (%.1f)x^2 +(%.1f)x +(%.1f)',p(1),p(2),p(3),p(4),p(5));
text(2,400,s)
xlabel('深度(m)');
ylabel('光阱力(pN)');
title('海水深度的变化对横向光阱力的影响');
